@model Setting
@{
    ViewData["Title"] = "Detail";
}

<div class="row">
    <div class="col-lg-6 h3 mb-4 text-gray-800">
        <h1>Setting Detail</h1>
    </div>
    <div class="col-lg-8 mx-auto">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title font-weight-bold">Logo</h5>
                <img class="card-img-top" style="width:50%" src="~/assets/images/@Model.Logo" alt="Card image cap">
                <h5 class="card-title font-weight-bold">Offer: @Html.Raw(Model.Offer)</h5>
                <h5 class="card-title font-weight-bold">Support Phone: @Model.SupportPhone</h5>
                <h5 class="card-title font-weight-bold">Phone: @Model.Phone</h5>
                <h5 class="card-title font-weight-bold">Email: @Model.Email</h5>
                <h5 class="card-title font-weight-bold">Work Day: @Model.WorkDay</h5>
                <a asp-action="Index" class="btn btn-primary">Go Back</a>
            </div>
        </div>
    </div>
</div>

