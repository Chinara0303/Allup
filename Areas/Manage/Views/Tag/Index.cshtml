@model IEnumerable<Tag>
@{
    ViewData["Title"] = "Index";
   
}

<div class="row">
    <div class="col-lg-10">
        <h1>Tags</h1>
    </div>
    <div class="col-lg-2">
        <a asp-action="Create" asp-area="Manage" class="btn btn-primary">Create</a>
    </div>
    <div class="col-lg-12 tagtable">
        @await Html.PartialAsync("_TagIndexPartial",Model);
    </div>

</div>
@section Script{
    <script src="~/manage/js/sweetalert2.all.min.js"></script>
    <script src="~/manage/js/custom.js" asp-append-version="true"></script>

}
@section Style{
    <link href="~/manage/css/sweetalert2.min.css">>

}


